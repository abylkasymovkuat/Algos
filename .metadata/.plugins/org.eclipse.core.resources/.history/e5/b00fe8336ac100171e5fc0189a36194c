package Gryadki;

import java.util.Scanner;

public class Graph {

    int [][] arr;
    int V;
    boolean [][] bArr;

    
    public Graph (int V){
        this.V = V;
        arr = new int[V+1][V+1];
        bArr = new boolean[V+1][V+1];
    }

    public void addEdge(int src, int dst){
        this.arr[src][dst] = 1;
        this.arr[dst][src] = 1;
    }

    public void printAll() {
    	for ( int i=1;i<=V;i++) {
    		for ( int j=1;j<=V;j++) 
    		{
    			System.out.print(arr[i][j]+ " ");
    		}
    		System.out.println();
    	}
    }
    public void printMat(){

        for ( int i =1;i<=this.V;i++)
        {
            System.out.print(i+ ":");
            for ( int j=1;j<=this.V;j++){
                if(arr[j][i]==1){
                    System.out.print(" "+j);
                }
            }
            System.out.println();
        }
    }

//    public boolean isTherePath(int u, int v){
